
@{
    ViewBag.Title = "Download";
}

<div>
    <h3>Скачать Программное обеспечение</h3>
    <form method="post">
        <input type="hidden" value="@ViewBag.PluginId" name="PluginId"/>
        <table>
            <tr>
                <td><p>Введите свое имя</p></td>
                <td><input type="text" name="Name" /></td>
            </tr>
            <tr>
                <td><p>Введите электронную почту</p></td>
                <td><input type="text" name="EMail" /></td>
            </tr>
            <tr>
                <td><input type="submit" name="Отправить" /></td>
                <td></td>
            </tr>
        </table>
    </form>

</div>
